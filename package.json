{
  "name": "michelson",
  "displayName": "Michelson Syntax",
  "description": "Syntax highlighting for the Michelson language and Morley extensions.",
  "version": "0.0.6",
  "publisher": "baking-bad",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.38.0"
  },
  "repository": {
    "url": "https://www.github.com/baking-bad/vscode-michelson-syntax"
  },
  "keywords": [
    "michelson",
    "morley",
    "tezos",
    "smart contracts",
    "syntax highlighting"
  ],
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "michelson",
        "aliases": [
          "Michelson",
          "michelson"
        ],
        "extensions": [
          ".tz"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "morley",
        "aliases": [
          "Morley",
          "morley"
        ],
        "extensions": [
          ".mtz"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "michelson",
        "scopeName": "source.michelson",
        "path": "./syntaxes/michelson.tmLanguage.json"
      },
      {
        "language": "morley",
        "scopeName": "source.morley",
        "path": "./syntaxes/morley.tmLanguage.json"
      }
    ]
  },
  "__metadata": {
    "id": "5b9daa6d-6292-42aa-81c0-76212c7bb01a",
    "publisherDisplayName": "Baking Bad",
    "publisherId": "274b8654-f936-4ada-869c-3e67e3f2e18c"
  }
}
